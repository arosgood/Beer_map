server <- function(input, output, session) {
clean = import('clean.csv')
data = clean
pal = colorFactor(c('Paired'),
domain = data$Category)
###filter
observe({
data = (subset(clean, ABV <= input$abv & IBU <= input$ibu & ((TRUE %in% (rowSums("Other Lager" == as.data.frame(input$categories)) > 0)))))
output$map <- renderLeaflet({
leaflet(data = data) %>%
addProviderTiles(providers$OpenStreetMap) %>% addCircleMarkers(~data$long, ~data$lat, radius = 3, color = ~pal(data$Category)) %>%
setView(lng = -30.85, lat = 37.45, zoom = 3) %>% addLegend(position = 'bottomright', pal = pal, values = data$Category, opacity = 0.7)
})
})
}
shinyApp(ui, server)
}
